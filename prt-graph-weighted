წონიანი ოროენტირებული გრაფის ბეჭდვის კოდი

void print(vector<pair<int,int>>adj[],int V){
   vector<pair<int,int>>::iterator it;
    for(int i=0;i<V;++i){
       for (it = adj[i].begin(); it != adj[i].end(); ++it) {
            int v = (*it).first;
            int wt = (*it).second;
            cout<<u<<"->"<<v<<" "<<wt<<endl;
        }
    }
}
